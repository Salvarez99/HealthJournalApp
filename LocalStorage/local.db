CREATE TABLE Appointments (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    date DATE,
    name TEXT,
    time_started TIME,
    time_ended TIME
);

CREATE TABLE Illnesses (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    date_started DATE,
    date_ended DATE,
    illness_name TEXT,
    time_started TIME,
    time_ended TIME
);

CREATE TABLE Medicines (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    dosage TEXT,
    dosage_schedule TEXT,
    frequency TEXT,
    medicine_name TEXT
);

CREATE TABLE Symptom_Checker (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    date DATE,
    symptom_name TEXT,
    time TIME
);

CREATE TABLE Test_and_Labworks (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    date DATE,
    test_lab_name TEXT,
    time TIME
);

CREATE TABLE User_Activity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    date DATE,
    journal_entry TEXT,
    time TIME
);